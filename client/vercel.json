{
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
      { "source": "/", "destination": "/src/pages/index.html" },
      { "source": "/dashboard", "destination": "/src/pages/dashboard/index.html" },
      { "source": "/signin", "destination": "/src/pages/signin.html" },
      { "source": "/signup", "destination": "/src/pages/signup.html" },
      { "source": "/(.*)", "destination": "/src/pages/index.html" }
    ]
  }
  